<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".settings_screen">

    <!-- TODO: Update blank fragment layout -->

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:layout_width="417dp"
        android:layout_height="94dp"
        app:menu="@menu/bottom_menu"
        tools:layout_editor_absoluteX="-6dp"
        tools:layout_editor_absoluteY="797dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="199dp"
        android:layout_height="88dp"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="48sp"
        tools:layout_editor_absoluteX="106dp"
        tools:layout_editor_absoluteY="91dp"
        tools:text="Settings" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

    <TextView
        android:id="@+id/EditText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="117dp"
        tools:layout_editor_absoluteY="254dp"
        tools:text="Edit Profile" />

    <TextView
        android:id="@+id/service_terms"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="117dp"
        tools:layout_editor_absoluteY="701dp"
        tools:text="Service Terms" />

    <TextView
        android:id="@+id/Privacy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="117dp"
        tools:layout_editor_absoluteY="638dp"
        tools:text="Privacy Policy" />

    <TextView
        android:id="@+id/deleteAccount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="91dp"
        tools:layout_editor_absoluteY="520dp"
        tools:text="Delete Account" />

    <TextView
        android:id="@+id/passwordChange"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="439dp"
        tools:text="Change Password" />

    <TextView
        android:id="@+id/signout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="Sign Out"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="345dp"
        tools:text="Sign Out" />

</androidx.constraintlayout.widget.ConstraintLayout>